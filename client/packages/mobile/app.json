{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "projectRoots": "",
      "sourceExts": ["js", "json", "ts", "tsx", "jsx"],
      "config": "metro.config.js"
    }
  }
}
